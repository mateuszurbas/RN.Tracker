{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@models/*": ["src/models/*"],
      "@navigator": ["src/navigator"],
      "@screens/*": ["src/screens/*"],
      "@theme": ["src/theme"],
      "@ts/*": ["src/types/*"],
      "@utils/*": ["src/utils/*"]
    }
  }
}
